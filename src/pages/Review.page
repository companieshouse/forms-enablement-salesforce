                <apex:outputPanel id="masterPanel">
                    <apex:outputPanel id="masterHeader" rendered="{!showMasterHeader}">
                        <h1 class="heading-xlarge">Review your application</h1>
                        <p class="lede">
                            Please confirm that the information you've provided us for {!companyName} is correct
                        </p>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!hasPageErrors}">
                        <div class="error-summary" role="group" aria-labelledby="error-summary-heading-example-1" tabindex="-1">
                            <h1 class="heading-medium error-summary-heading" id="error-summary-heading-example-1">
                                An error has occurred.
                            </h1>
                            <p>
                            </p>
                            <ul class="error-summary-list">
                                <li>
                                    <a href="#">{!currentErrorMessage}</a>
                                </li>
                            </ul>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel id="standardPanel" rendered="{!standardPanelView}">
                        <h1 class="heading-xlarge">
                            {!headerText1}
                        </h1>
                        <p class="lede">{!displayText1}
                        </p>
                        <table class="check-your-answers">
                            <thead>
                                <tr>
                                    <th colspan="3">
                                        <h2 class="heading-medium">Request details</h2>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>Company name</td>
                                    <td>{!companyName}</td>
                                </tr>
                                <tr>
                                    <td>Company number</td>
                                    <td>{!companyNumber}</td>
                                </tr>
                                <tr>
                                    <td>Registered Office Address</td>
                                    <td>
                                        <apex:outputText escape="false" value="{!companyAddress}" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>Approved by</td>
                                    <td>
                                        <apex:repeat value="{!companyDirectors}" var="Director" id="theRepeat">
                                            <apex:outputText value="{!Director.Name}" id="theValue" />
                                            <br/>
                                        </apex:repeat>
                                    </td>
                                </tr>
                                <tr>
                                    <td>Presenter details</td>
                                    <td>{!presenterEmail}
                                        <br/>{!presenterName}
                                        <br/> {!presenterPhone}
                                    </td>
                                </tr>
                            </tbody>
                        </table>
